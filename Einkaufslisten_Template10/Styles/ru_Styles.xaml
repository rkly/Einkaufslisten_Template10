<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Einkaufslisten_Template10.Styles"
    xmlns:controls="using:Template10.Controls">


    <Style TargetType="Button" x:Key="ru_btn_style">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF63506E" Offset="0.996"/>
                    <GradientStop Color="#C53B0859"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>
    <SolidColorBrush x:Key="ru_solidColorBrush_RelativePanel" Color="#FF110B81" Opacity="0.625"/>
</ResourceDictionary>
